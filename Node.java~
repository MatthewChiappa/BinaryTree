package hw11;

public class Node {
  
  Object num;
  Node leftNode;
  Node rightNode;
  
  public Node(Object num, Node leftNode, Node rightNode) {
    
    this.num = num;
    this.setLeft(leftNode);
    this.setRight(rightNode);
    
  }
  
  public void setLeft(Node leftNode) {
    this.leftNode = leftNode; 
  }
  
  public void setRight(Node rightNode) {
    this.rightNode = rightNode; 
  }
  
  public Node getLeft() {
    return leftNode;
  }
  
  public Node getRight() {
    return rightNode;
  }
  
}